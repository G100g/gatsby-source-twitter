{
    "name": "gatsby-source-twitter",
    "version": "2.0.0",
    "description": "Fetch data from Twitter API",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/G100g/gatsby-source-twitter"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore __tests__",
        "watch": "babel -w src --out-dir . --ignore __tests__",
        "prepublish": "cross-env NODE_ENV=production npm run build",
        "test": "jest"
    },
    "keywords": [
        "gatsby",
        "gatsbyjs",
        "gatsby-plugin",
        "twitter"
    ],
    "files": [
        "*.js"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "babel-preset-gatsby-package": "^0.1.3",
        "cross-env": "^5.1.0",
        "jest": "^24.0.0",
        "mockery": "^2.1.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.3.1",
        "twitter": "^1.7.1"
    },
    "peerDependencies": {
        "gatsby": "^2.0.88"
    }
}
